{ $as_echo "$as_me:${as_lineno-$LINENO}: checking for domain information" >&5
$as_echo_n "checking for domain information... " >&6; }
HOSTDOMAIN=""
n=5
i=1
while [ $i -le $n ] ; do
  case $i in
    4)
      if hostname 2> /dev/null 1>&2 ; then
         HOSTDOMAIN=`hostname -f 2> /dev/null` || HOSTDOMAIN=""
         status=$?
      else
         status=-1
      fi
      ;;
    2)
      if which hostname 2> /dev/null 1>&2 ; then
         HOSTDOMAIN=`hostname`.`hostname -d 2> /dev/null` || HOSTDOMAIN=""
         status=$?
      else
         status=-1
      fi
      ;;
    3)
      if which dnsdomainname 2> /dev/null 1>&2 ; then
         HOSTDOMAIN=`hostname`.`dnsdomainname -d 2> /dev/null` || HOSTDOMAIN=""
         status=$?
      else
         status=-1
      fi
      ;;
    1)
      if which nslookup 2> /dev/null 1>&2 ; then
         HOSTDOMAIN=`nslookup -silent $HOST 2> /dev/null | grep Name | head -n 1 | awk '{print $2}'` || HOSTDOMAIN=""
         status=$?
      else
         status=-1
      fi
      ;;
    5)
      if which host 2> /dev/null 1>&2 ; then
         HOSTDOMAIN=`host $HOST 2> /dev/null | grep -v "not found" | awk '{print $1}'` || HOSTDOMAIN=""
         status=$?
      else
         status=-1
      fi
      ;;
  esac
  if test -z "$HOSTDOMAIN" ;then
     i=`expr $i + 1`
  else
     i=`expr $n + 1`
  fi
done

if test -z "$HOSTDOMAIN" ; then
  HOSTDOMAIN=unknown
fi

