        dj = 0. &
          + sig_CH2Cl2(0) * fint(j,k,0) &
          + sig_CH2Cl2(1) * fint(j,k,1) &
          + sig_CH2Cl2(2) * fint(j,k,2)

        jval_2d(ip_CH2Cl2)%ptr(iu0(j),k) = REAL(MAX(0.0, dj*fj_corr(j,8)),dp)
      ENDDO
    ENDDO

  END SUBROUTINE jval_cal_CH2Cl2

