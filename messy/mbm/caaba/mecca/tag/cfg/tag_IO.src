; = NOx ====================================================
; ----------------------------------------------------------
;<G3103> NO + O3  = NO2 + O2
  G3103 O3 = O2 NO2
  G3103 NO =    NO2
; ----------------------------------------------------------
;<G3105> NO2 + O3P = NO + O2
  G3105 NO2 = NO O2
  G3105 O3P =    O2
; ----------------------------------------------------------
;<G3106> NO2 + O3  = NO3 + O2
; NO2 accounts 2/3 of NO3
; O3           1/3 of NO3
  G3106 NO2 = 2 NO3
  G3106 O3  = 1 NO3 O2
; ----------------------------------------------------------
;<G3201> NO + HO2 = NO2 + OH
  G3201 NO  = NO2
  G3201 HO2 = NO2 OH
; ----------------------------------------------------------
;<G3204> NO3 + HO2 = NO2 + OH + O2
  G3204 NO3 = NO2 OH
  G3204 HO2 = O2
; ----------------------------------------------------------
;<G3205> HONO + OH  = NO2 + H2O
  G3205 HONO = NO2
  G3205 OH = H2O
; ----------------------------------------------------------
;<G3206> HNO3 + OH  = H2O + NO3
  G3206 HNO3 = NO3
  G3206 OH = H2O
; ----------------------------------------------------------
;<G3208>  HNO4 + OH  = NO2 + H2O + O2
  G3208 HNO4 = NO2 O2
  G3208 OH = H2O
; ----------------------------------------------------------
; # eval2.3 N+H addition
; ----------------------------------------------------------
;<G3213>  NH2   + NO      = HO2 + OH + N2 : {%TrGN}   1.92E-12*((temp/298.)**(-1.5)); {&2415}
; ?? not balanced, NO gets converted to OH, see e.g. 1993.JPC97.Stephens,etal (pp 8944)
  G3213 NO = OH
  G3213 O2 = HO2
; ----------------------------------------------------------
;<G3217>  NH2O  + O3      = NH2 + O2      : {%TrGN}   1.2E-14; {&2415}
; ?? not balanced
; assuming 4O is lost in 2xO2
  G3217 NH2O = O2
  G3217   O3 = O2
; ----------------------------------------------------------
;<G3219>  HNO   + OH      = NO + H2O      : {%TrGN}   8.0E-11*EXP(-500./temp); {&2415}
  G3219 HNO = NO
  G3219  OH = H2O
; ----------------------------------------------------------
;<G3220>  HNO   + NHOH    = NH2OH + NO    : {%TrGN}   1.66E-12*EXP(-1500./temp); {&2415}
; H-abstraction?
  G3220  HNO = NO
  G3220 NHOH = NH2OH
; ----------------------------------------------------------
;<G3221>  HNO   + NO2     = HONO + NO     : {%TrGN}   1.0E-12*EXP(-1000./temp); {&2415}
; H-abstraction?
  G3221 HNO = NO
  G3221 NO2 = HONO
; ----------------------------------------------------------
;<G3222>  NHOH  + OH      = HNO + H2O     : {%TrGN}   1.66E-12; {&2415}
; H-abstraction?
  G3222 NHOH = HNO
  G3222   OH = H2O
; ----------------------------------------------------------
;<G3223>  NH2OH + OH      = NHOH + H2O    : {%TrGN}   4.13E-11*EXP(-2138./temp); {&2415}
; H-abstraction?
  G3223 NH2OH = NHOH
  G3223    OH = H2O
; ----------------------------------------------------------
;<G3224>  HNO   + O2      = HO2 + NO      : {%TrGN}   3.65E-14*EXP(-4600./temp); {&2415}
; H-abstraction?
  G3224 HNO = NO
  G3224  O2 = HO2


