//!   RHET_N2O5_TropLiq() ; {N2O5   + H2O(c) = 2HNO3(c)}
//      REAL(dp) FUNCTION RHET_N2O5_TropLiq(pres,temp)
//      REAL(dp), INTENT(IN) :: pres,temp
//      REAL(dp),PARAMETER :: M_N2O5 =  (2*14+5*16.)/6.02E23 *1E-3 ! N2O5 mass, kg/molec
//      REAL(dp),PARAMETER :: PI =  3.14159265
//      REAL(dp),PARAMETER :: G_N2O5_I =  0.02
//      REAL(dp)           :: G_N2O5_L,SGS_MIX_N2O5
//      REAL(dp)           :: DG,ZCC,ZDG,KT_LIQ_N2O5,KT_ICE_N2O5
//      REAL(dp)           :: R_CLOUD,R_ICE,SAD_CLOUD,SAD_ICE
//      REAL(dp)           :: C_THERMAL_N2O5
//
//      ! The surface area density for ice particles in now linked to
//      DG=0.1E5/Pres     !simple approximation for diffusion coeff. [cm2/s]
//      C_THERMAL_N2O5 = SQRT (8*1.38E-23 *TEMP /(PI* M_N2O5 ) )  * 1E2 ! m/s -> cm/sec
//
//      G_N2O5_L = 2.7E-5*EXP(1800./TEMP)
//      SGS_MIX_N2O5=ZCC ! Basic SGS Scaling factor
//      KT_LIQ_N2O5 =1./(R_CLOUD/ZDG  + 4./(C_THERMAL_N2O5*G_N2O5_L))
//      KT_ICE_N2O5 =1./(R_ICE/ZDG    + 4./(C_THERMAL_N2O5*G_N2O5_I))
//      RHET_N2O5_TropLiq=(KT_LIQ_N2O5*SAD_CLOUD+KT_ICE_N2O5*SAD_ICE)* SGS_MIX_N2O5
//
//
//      END FUNCTION RHET_N2O5_TropLiq
