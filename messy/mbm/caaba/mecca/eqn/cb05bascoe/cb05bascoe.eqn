//!   RHET_N2O5_TropAer() ; {N2O5   + H2O(c) = 2HNO3(c)}
//      REAL(dp) FUNCTION RHET_N2O5_TropAer(pres,temp)
//      REAL(dp), INTENT(IN) :: pres,temp
//      REAL(dp),PARAMETER :: M_N2O5 =  (2*14+5*16.)/6.02E23 *1E-3 ! N2O5 mass, kg/molec
//      REAL(dp),PARAMETER :: PI =  3.14159265
//      REAL(dp), PARAMETER    :: G_N2O5_SS=0.02
//      REAL(dp), PARAMETER    :: G_N2O5_DD=0.01
//      REAL(dp), PARAMETER    :: G_N2O5_OM=0.02
//      REAL(dp), PARAMETER    :: G_N2O5_BC=0.01
//      REAL(dp), PARAMETER    :: G_N2O5_SO4=0.02
//      REAL(dp) :: SAD_SS,R_SS
//      REAL(dp) :: SAD_DD,R_DD
//      REAL(dp) :: SAD_OM,R_OM
//      REAL(dp) :: SAD_BC,R_BC
//      REAL(dp) :: SAD_SO4,R_SO4
//      REAL(dp) :: DG, C_THERMAL_N2O5, K_AER_N2O5
//      ! The surface area density for ice particles in now linked to
//      DG=0.1E5/Pres     !simple approximation for diffusion coeff. [cm2/s]
//      C_THERMAL_N2O5 = SQRT (8*1.38E-23 *TEMP /(PI* M_N2O5 ) )  * 1E2 ! m/s -> cm/sec
//
//      K_AER_N2O5=           SAD_SS*(1./((R_SS/DG)+(4./(C_THERMAL_N2O5*G_N2O5_SS))))
//      K_AER_N2O5=K_AER_N2O5+SAD_DD*(1./((R_DD/DG)+(4./(C_THERMAL_N2O5*G_N2O5_DD))))
//      K_AER_N2O5=K_AER_N2O5+SAD_OM*(1./((R_OM/DG)+(4./(C_THERMAL_N2O5*G_N2O5_OM))))
//      K_AER_N2O5=K_AER_N2O5+SAD_BC*(1./((R_BC/DG)+(4./(C_THERMAL_N2O5*G_N2O5_BC))))
//      K_AER_N2O5=K_AER_N2O5+SAD_SO4*(1./((R_SO4/DG)+(4./(C_THERMAL_N2O5*G_N2O5_SO4))))
//
//      RHET_N2O5_TropAer=K_Aer_N2O5
//
//      END FUNCTION RHET_N2O5_TropAer
