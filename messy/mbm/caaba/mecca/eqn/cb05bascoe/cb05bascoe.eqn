//!   RHET_HO2_TropLiq() ; {HO2   + HO2 + H2O(c) = H2O2(c)}
//      REAL(dp) FUNCTION RHET_HO2_TropLiq(pres,temp,cHO2)
//      REAL(dp), INTENT(IN) :: temp  ! temperature
//      REAL(dp), INTENT(IN) :: pres      ! air pressure (Pa)
//      REAL(dp), INTENT(IN) :: cHO2      ! HO2 concentration [molecules/cm3]
//      REAL(dp),PARAMETER :: M_HO2 =  (1.+2*16.)/6.02E23 *1E-3 ! HO2 mass, kg/molec
//      REAL(dp),PARAMETER :: PI =  3.14159265
//      REAL(dp),PARAMETER :: G_HO2_I =  0.025
//      REAL(dp),PARAMETER :: G_HO2_L =  0.01
//      REAL(dp)           :: C_THERMAL_HO2,SGS_MIX_HO2
//      REAL(dp)           :: ZCC,ZDG,KT_LIQ_HO2,KT_ICE_HO2,K_LIQ_HO2
//      REAL(dp)           :: R_CLOUD,R_ICE,CHO2MAX,SAD_CLOUD,SAD_ICE
//
//      C_THERMAL_HO2  = SQRT (8*1.38E-23 *TEMP /(PI* M_HO2  ) )  * 1E2 ! m/s -> cm/sec
//      SGS_MIX_HO2=ZCC ! Basic SGS Scaling factor
//      KT_LIQ_HO2 =1./(R_CLOUD/ZDG  + 4./(C_THERMAL_HO2*G_HO2_L))
//      KT_ICE_HO2 =1./(R_ICE/ZDG    + 4./(C_THERMAL_HO2*G_HO2_I))
//
//      !change heterogeneous reaction rate into a second-order reaction (HO2+HO2)
//      !through division by HO2 [molec/cm3]
//      cHO2max=max(cHO2,1E5)
//
//      K_LIQ_HO2=(KT_LIQ_HO2*SAD_CLOUD+KT_ICE_HO2*SAD_ICE) * SGS_MIX_HO2
//      RHET_HO2_TropLiq=K_Liq_HO2/cHO2max
//
//      END FUNCTION RHET_HO2_TropLiq
//
//
//!   RHET_HO2_TropAer() ; {HO2   + HO2 + H2O(c)= H2O2 }
//      REAL(dp) FUNCTION RHET_HO2_TropAer(pres,temp,cHO2)
//       REAL(dp), INTENT(IN) :: temp  ! temperature
//       REAL(dp), INTENT(IN) :: pres      ! air pressure (Pa)
//       REAL(dp), INTENT(IN) :: cHO2      ! HO2 concentration [molecules/cm3]
//      REAL(dp),PARAMETER :: M_HO2 =  (1.+2*16.)/6.02E23 *1E-3 ! HO2 mass, kg/molec
//      REAL(dp),PARAMETER :: PI =  3.14159265
//      REAL(dp), PARAMETER    :: G_HO2_SS=0.01
//      REAL(dp), PARAMETER    :: G_HO2_DD=0.1
//      REAL(dp), PARAMETER    :: G_HO2_OM=0.001
//      REAL(dp), PARAMETER    :: G_HO2_BC=0.01
//      REAL(dp), PARAMETER    :: G_HO2_SO4=0.01
//      REAL(dp)               :: cHO2max
//      REAL(dp)               :: C_THERMAL_HO2,ZM_HO2,K_AER_HO2,DG
//      REAL(dp)               :: SAD_SS,R_SS
//      REAL(dp)               :: SAD_DD,R_DD
//      REAL(dp)               :: SAD_OM,R_OM
//      REAL(dp)               :: SAD_BC,R_BC
//      REAL(dp)               :: SAD_SO4,R_SO4
//
//      DG=0.1E5/Pres     !simple approximation for diffusion coeff. [cm2/s]
//      C_THERMAL_HO2 = SQRT (8*1.38E-23 *TEMP /(PI* ZM_HO2 ) )  * 1E2 ! m/s -> cm/sec
//
//      K_AER_HO2=          SAD_SS *(1./((R_SS/DG)+(4./(C_THERMAL_HO2*G_HO2_SS))))
//      K_AER_HO2=K_AER_HO2+SAD_DD *(1./((R_DD/DG)+(4./(C_THERMAL_HO2*G_HO2_DD))))
//      K_AER_HO2=K_AER_HO2+SAD_OM *(1./((R_OM/DG)+(4./(C_THERMAL_HO2*G_HO2_OM))))
//      K_AER_HO2=K_AER_HO2+SAD_BC *(1./((R_BC/DG)+(4./(C_THERMAL_HO2*G_HO2_BC))))
//      K_AER_HO2=K_AER_HO2+SAD_SO4*(1./((R_SO4/DG)+(4./(C_THERMAL_HO2*G_HO2_SO4))))
//
//      !change heterogeneous reaction to second-order reaction (HO2+HO2)
//      !through division by HO2 [molec/cm3]
//      cHO2max=max(cHO2,1E5)
//
//      RHET_HO2_TropAer=K_Aer_HO2/cHO2max
//
//      END FUNCTION RHET_HO2_TropAer
//
      REAL(dp) FUNCTION K_HO2_HO2 (temp,cair,cH2O)
       REAL(dp), INTENT(IN) :: temp  ! temperature
       REAL(dp), INTENT(IN) :: cair      ! air concentration [molecules/cm3]
       REAL(dp), INTENT(IN) :: cH2O      ! H2O concentration [molecules/cm3]
       K_HO2_HO2=(3.5E-13*EXP(430./temp)+1.7E-33*EXP(1000./temp)*cair)* &
          &       (1.+1.4E-21*EXP(2200./temp)*cH2O)
      END FUNCTION K_HO2_HO2
//
//
//
//!   RHET_NO3_TropAer() ; {NO3   + H2O(c) = Aerosol (or HNO3??)}
//      REAL(dp) FUNCTION RHET_NO3_TropAer(pres,temp)
//      REAL(dp), INTENT(IN) :: pres,temp
//      REAL(dp),PARAMETER :: M_NO3 =  (2*14+5*16.)/6.02E23 *1E-3 ! N2O5 mass, kg/molec
//      REAL(dp),PARAMETER :: PI =  3.14159265
//      REAL(dp), PARAMETER    :: ZG_NO3=0.01
//      REAL(dp)  :: DG,KTNO3_AER,JPRB,ZR_EFF,ZDG
//      REAL(dp)  :: SAD_AER,SAD_SS,SAD_OM,SAD_BC,SAD_SO4
//
//      ! The surface area density for ice particles in now linked to
//      DG=0.1E5/Pres     !simple approximation for diffusion coeff. [cm2/s]
//
//      KTNO3_AER=1._JPRB/((ZR_EFF/ZDG)+(4._JPRB/(3.0E4_JPRB*ZG_NO3)))
//      SAD_AER = SAD_SS + SAD_OM + SAD_BC + SAD_SO4
//
//      RHET_NO3_TropAer=KTNO3_AER*SAD_AER
//     END FUNCTION RHET_NO3_TropAer

