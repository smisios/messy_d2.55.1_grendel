// based on jam002b_20170510mgs
#INLINE F90_GLOBAL
  REAL(DP) :: kHO2_HO2
  REAL(DP) :: kNO2NO3, kN2O5
  REAL(DP) :: kHNO3_OH
  REAL(DP) :: kNO2HO2, kHNO4
  REAL(DP) :: kCO_OH, kCO_OH_inf, kCO_OH_o, kCO_OH_term1, kCO_OH_term2
  REAL(DP) :: kNO2CH3O2, kCH3O2NO2
  REAL(DP) :: k_CH3CO3_NO2, kPAN
  REAL(DP) :: k_MACO3_NO2, kMPAN
  REAL(DP) :: kClOClO, kCl2O2
#ENDINLINE {above lines go to messy_mecca_kpp_global}

#INLINE F90_RCONST
  ! start of executable statements
  ! define some rate constants here if the expressions are too long
  ! for kpp or if they are used more than once
  kHO2_HO2     = (3.0E-13*EXP(460./temp)+2.1E-33*EXP(920./temp)*cair)* &
                 (1.+1.4E-21*EXP(2200./temp)*C(ind_H2O))
  kNO2NO3      = k_3rd(TEMP,cair,2.000e-30,4.4,1.400e-12,0.7,0.6)
  kN2O5        = kNO2NO3 * 3.333e26 * EXP(-10991./TEMP)
  kHNO3_OH     = 2.4E-14*EXP(460./temp) + 1./ &
                 ( 1./(6.5E-34*EXP(1335./temp)*cair) + &
                 1./(2.7E-17*EXP(2199./temp)) )
  kNO2HO2      = k_3rd(TEMP,cair,2.000e-31,3.4,2.900e-12,1.1,0.6)
  kHNO4        = kNO2HO2 * EXP(-10900./TEMP) / 2.1e-27
  ! CO + OH start
  kCO_OH_inf   = 2.1e+09 * (TEMP/ 300.)**(6.1)
  kCO_OH_o     = 1.5e-13 * (TEMP/ 300.)**(0.6)
  kCO_OH_term1 = kCO_OH_o / ( (kCO_OH_inf / cair) )
  kCO_OH_term2 = kCO_OH_o / (1. + kCO_OH_term1)
  kCO_OH_term1 = LOG10( kCO_OH_term1 )
  kCO_OH_term1 = 1. / (1. + kCO_OH_term1*kCO_OH_term1)
  kCO_OH       = kCO_OH_term2 * (0.6)**kCO_OH_term1
  ! CO + OH end
  kNO2CH3O2    = k_3rd(TEMP,cair,1.000e-30,4.8,7.200e-12,2.1,0.6)
  kCH3O2NO2    = kNO2CH3O2 * EXP(-11234./TEMP) / 9.5e-29
  k_CH3CO3_NO2 = k_3rd(TEMP,cair,2.700e-28,7.1,1.200e-11,0.9,0.6)
  kPAN         = k_CH3CO3_NO2 * 1.111e28 * EXP(-14000./TEMP)
  k_MACO3_NO2  = k_3rd(TEMP,cair,2.700e-28,7.1,1.200e-11,0.9,0.3)
  kMPAN        = k_MACO3_NO2 * 1.111e28 * EXP(-14000./TEMP)
  kClOClO      = k_3rd(TEMP,cair,1.600e-32,4.5,3.000e-12,2.,0.6)
  kCl2O2       = kClOClO / (1.3e-27 * EXP(8744./TEMP))
#ENDINLINE {above lines go into the SUBROUTINES UPDATE_RCONST and UPDATE_PHOTO}

