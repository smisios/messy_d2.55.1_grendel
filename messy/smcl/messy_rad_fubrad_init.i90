       IDX_HRZ = IDX_HRZ_143
       IDX_HAR = IDX_HAR_143
       IDX_HUG = IDX_HUG_143
       IDX_ADD = IDX_ADD_143
       IDX_CHA = IDX_CHA_143
       nsrb    = nsrb_k
       nchap   = nchap_106   
       ntr_max = ntr_k
    CASE(144)
       IDX_SRB = IDX_SRB_144
       IDX_HRZ = IDX_HRZ_144
       IDX_HAR = IDX_HAR_144
       IDX_HUG = IDX_HUG_144
       IDX_ADD = IDX_ADD_144
       IDX_CHA = IDX_CHA_144
       nsrb    = nsrb_km
       nchap   = nchap_106
       ntr_max = ntr_km
    CASE(146)
       IDX_SRB = IDX_SRB_146
       IDX_HRZ = IDX_HRZ_146
       IDX_HAR = IDX_HAR_146
       IDX_HUG = IDX_HUG_146
       IDX_ADD = IDX_ADD_146
       IDX_CHA = IDX_CHA_146
       nsrb    = nsrb_st
       nchap   = nchap_106  
    CASE DEFAULT
       WRITE(*,*) substr, ': ERROR IN CHOICE OF NBANDS '// &
                          ' ! NBANDS = ',nbands,' NOT VALID; ONLY 49, 55, OR 106.'
       RETURN
    END SELECT
!
    status = 0
    RETURN
