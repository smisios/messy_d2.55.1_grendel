        PRINT*,'ERROR IN WLAMCH. EPS < ',Eps
        RETURN
10      Eps = Eps*2
        i = i- 1      
      END IF

      WLAMCH = Eps

      END FUNCTION WLAMCH
 
      SUBROUTINE WLAMCH_ADD( A, B, Suma)
!        USE messy_gmxe_aerchem_kpp_Precision
      
      REAL(kind=dp) A, B, Suma
      Suma = A +  B

