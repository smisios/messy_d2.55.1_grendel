   ros_E(1) =  0.5E+00_dp
   ros_E(2) = - 0.29079558716805469821718236208017E+01_dp
   ros_E(3) =  0.22354069897811569627360909276199E+00_dp
!  ~~~> ros_ELO = estimator of local order -  the minimum between the
!      main and the embedded scheme orders plus 1
   ros_ELO = 3.0_dp
