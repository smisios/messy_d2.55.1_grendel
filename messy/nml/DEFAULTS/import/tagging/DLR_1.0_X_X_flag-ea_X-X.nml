infile    = "$INPUTDIR_MESSY/tagging/DLR_1.0_X_X_flag-ea_X-X.nc", 
i_latm    = 'lat',
i_latr    = -90.0,90.0,
i_lonm    = 'lon',
i_lonr    = 0.,360.0,
i_timem   = 'time',
var        ='flag|longname=flag field for east asia', 
/
